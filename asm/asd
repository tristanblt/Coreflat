.name "Querelle des Noyaux"
.comment "La Querelle des Noyaux est une evenement historique prenant place initialement dans nos coeurs"

		sti r1 %:live_0 %1
		ld %4 r4

chossu_0:	ldi r2 %0 r3
		sti r3 r2 %37
		add r2 r4 r2

live_0:		live %1
		xor r2 %42 r16
		zjmp %:chossu_0
		ld %0 r2
		fork %:chossu_0
